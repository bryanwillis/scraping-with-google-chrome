{
	"manifest_version": 2,
	
	
	"name": "ThinApp Scraper",
	"description": "Scraps VMware® ThinApp® and ThinApp Factory® webpage",
	"version": "1.0",

	"permissions": [
		"tabs",
		"http://*/*"
	],
	
	"content_scripts": [
		{
			"matches": ["http://communities.vmware.com/*"],
			"js": ["jquery-2.0.0.min.js", "inject.js"]
		}
	]
}